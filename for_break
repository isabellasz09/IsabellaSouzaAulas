presentes = ['John Snow','Jesse Pickman','Aria Stark','Tyrion Lannister']
chamada= 'Aria Stark'
for pessoa in presentes:
    if pessoa == chamada:
        print('{} est치 presente.'.format(chamada))
        break
    else:
        print('S칩 um print para mostrar que o for passou por esse pessoa: ' +str(pessoa))


